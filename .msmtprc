defaults
auth           on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        ~/.msmtp.log

account        personal
host           smtp.fastmail.com
port           587
tls_starttls   on
from           evan@evanriley.com
user           evan@evanriley.com
auth           plain
passwordeval   "gpg -q --for-your-eyes-only --no-tty -d ~/.authinfo.asc 2>/dev/null | awk '/machine imap.fastmail.com login evan@evanriley.com/ {print $NF}'"

account default : personal
